#

_roshi () {
   _arguments : \
      '1: :_files' \
      '2:subcommand:((
         newlog\:"create a new workoutlog"
         add\:"add new training session"
         show\:"show a certain training sessions"
         list\:"list all logged workouts"
      ))'
}

_roshi
